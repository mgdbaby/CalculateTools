<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="line_fill" format="boolean" />

    <declare-styleable name="TitleBarView">
        <attr name="left_visible" format="boolean" />
        <attr name="left_img" format="reference|color" />
        <attr name="left_text" format="string" />
        <attr name="right_visible" format="boolean" />
        <attr name="right_img" format="reference|color" />
        <attr name="right_text" format="string" />
        <attr name="title_text" format="string" />
    </declare-styleable>

    <declare-styleable name="ItemMoreView">
        <attr name="title" />
        <attr name="line_fill" />
    </declare-styleable>

    <declare-styleable name="ItemMoreDotView">
        <attr name="title" />
        <attr name="line_fill" />
        <attr name="show_tip" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ItemMoreDescView">
        <attr name="title" />
        <attr name="line_fill" />
        <attr name="content" format="string" />
    </declare-styleable>

    <declare-styleable name="ItemSwitchView">
        <attr name="title" />
        <attr name="line_fill" />
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SearchBarAnimView">
        <attr name="search_text" format="string"/>
        <attr name="enable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RippleSpreadView">
        <attr name="rsv_innerCircleColor" format="color" />
        <attr name="rsv_innerSize" format="dimension" />
        <attr name="rsv_innerAnimDuration" format="integer" />
        <attr name="rsv_outCircleColor" format="color" />
        <attr name="rsv_outSize" format="dimension" />
        <attr name="rsv_outAnimDuration" format="integer" />
    </declare-styleable>
</resources>
